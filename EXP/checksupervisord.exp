#!/usr/bin/expect -f

#Usage sshsudologin.expect <host> <ssh user> <ssh password>

spawn ssh [lindex $argv 1]@[lindex $argv 0]

set timeout 2

expect "yes/no" { send "yes\r" }

expect "$ " { send -- "hostname\r" }
expect "$ " { send -- "sudo ps -ef | grep -i supervisord | grep -v grep\r" }
expect "$ " { send -- "exit\r" }
